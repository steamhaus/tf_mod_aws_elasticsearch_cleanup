{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "LambdaLogCreation",
      "Effect": "Allow",
      "Action": ["logs:*"],
      "Resource": "arn:aws:logs:*:*:*"
    },
    {
      "Sid": "ESPermission",
      "Effect": "Allow",
      "Action": [
        "es:*"
      ],
      "Resource": "*"
    }
  ]
}